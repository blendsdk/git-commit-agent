{
    "name": "@blendsdk/git-commit-agent",
    "version": "1.0.8",
    "main": "dist/index.js",
    "type": "module",
    "bin": {
        "git-commit-agent": "./dist/index.js"
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE",
        "ENHANCEMENTS.md",
        "COMMIT_PROMPT_GUIDE.md"
    ],
    "scripts": {
        "build": "tsc",
        "dev": "rm -fR ./dist && tsc --watch",
        "start": "node ./dist/index.js",
