    },
    "engines": {
        "node": ">=18.0.0"
    },
    "dependencies": {
        "@langchain/core": "^1.0.2",
        "@langchain/openai": "^1.0.0",
        "@types/node": "^24.9.2",
        "dotenv": "^17.2.3",
        "execa": "^9.6.0",
        "langchain": "^1.0.2",
        "tslib": "^2.8.1",
        "typescript": "^5.9.3",
